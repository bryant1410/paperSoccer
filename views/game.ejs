<div id="game-area">
    <div id="game">
        <div id="score-container">
            <span id="player-a-move-icon">
                <img src="../img/football.png"/>
            </span>
            <span id="player-a-nick">gracz A</span>
            <span id="score">0 : 0</span>
            <span id="player-b-nick">gracz B</span>
            <span id="player-b-move-icon">
                <img src="../img/football.png"/>
            </span>
        </div>
        <div id="game-buttons">
            <span id="time">0:30</span>
            <!--<button id="leave-game">Opuść grę</button>-->
        </div>
        <canvas id="game-canvas">
        </canvas>
    </div>
    <div id="game-chat-area">
        <textarea id="game-chat" disabled="disabled">
        </textarea>
        <div id="game-chat-input-box">
            <input type="text" id="game-chat-input" />
            <button id="game-chat-input-button">Wyślij</button>
        </div>
    </div>

</div>
